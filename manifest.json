{
   "author": "Soroboruo",
   "background": {
      "persistent": false,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "img/icon19.png",
         "38": "img/icon38.png"
      },
      "default_title": "Active ou Desactive le plein ecran"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/style.css" ],
      "js": [ "js/content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Auto Fullscreen for noco.tv.",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   },
   "manifest_version": 2,
   "name": "OtoNoco",
   "offline_enabled": true,
   "permissions": [ "http://*/*", "https://*/*" ],
   "version": "2.3"
}
